<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>Patrick Schubert</Authors>
    <Company />
    <Product>PS Fritz!Box API</Product>
    <Description>API for accessing the TR-064 interface of AVM Fritz!Box devices.</Description>
    <Copyright>Patrick Schubert</Copyright>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <PackageLicenseUrl>https://github.com/inflames2k/PS.FritzBox.API/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/inflames2k/PS.FritzBox.API</PackageProjectUrl>
    <RepositoryType>Git</RepositoryType>
    <AssemblyVersion>1.2.1.0</AssemblyVersion>
    <Version>1.2.2</Version>
    <RepositoryUrl>https://github.com/inflames2k/PS.FritzBox.API.git</RepositoryUrl>
    <FileVersion>1.2.1.0</FileVersion>
    <PackageTags>AVM, Fritz!Box, Fritz.Box, TR064, UPnP</PackageTags>
    <PackageReleaseNotes>Bugfixes
New interface WANDSLInterfaceConfigClient
New interface WLANConfigurationClient2 for configuring 5 GHz WLAN
New interface WLANConfigurationClient3 for configuring guest WLAN</PackageReleaseNotes>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DocumentationFile>bin\Debug\netstandard1.6\PS.FritzBox.API.xml</DocumentationFile>
  </PropertyGroup>

</Project>
